def cek_angka(angka1,angka2,angka3):
    if angka1 != angka2 and angka2 != angka3 and angka3 != angka1:
        if angka1 + angka2 == angka3:
            print("True")
        elif angka2 + angka3 == angka1:
            print("True")
        elif angka1 + angka3 == angka2:
            print("True")
        else:
            print("False")
    else:
        print("False")

angka1 = int(input())
angka2 = int(input())
angka3 = int(input())

cek_angka(angka1,angka2,angka3)
